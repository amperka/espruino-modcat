    "express": "^4.17.3",
    "object-assign": "^4.1.0",
    "request-json": "^0.5.6"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "prettier": "^1.18.2",
    "prettylint": "^1.0.0",
    "uglify-es": "^3.3.9"
  }
}
